{
  "name": "league_app",
  "version": "0.0.1",
  "description": "LOL matchup tracking",
  "main": "./bld/app.js",
  "module": "./src/app.js",
  "scripts": {
    "eslint": "eslint --ext=js ./src",
    "js": "babel src --out-dir bld",
    "assets": "cp -R src/public/ bld/public/",
    "html": "cp -R src/views/ bld/views/",
    "build": "npm run js && npm run html && npm run assets",
    "prestart": "npm run build",
    "start": "node ./bld/app.js"
  },
  "author": "Connor Phee",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "body-parser": "^1.17.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "ejs": "^2.3.4",
    "eslint": "^4.8.0",
    "eslint-config-node": "^1.6.0",
    "express": "^4.15.4",
    "express-session": "^1.11.3",
    "geocoder": "^0.2.2",
    "method-override": "^2.3.5",
    "moment": "^2.21.0",
    "mongoose": "^4.11.10",
    "mongoose-unique-validator": "^1.0.6",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^3.1.0",
    "request": "^2.65.0"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^2.6.0",
    "prettier": "^1.11.1",
    "prettier-eslint": "^8.8.1"
  }
}
